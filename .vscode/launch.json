{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Chrome",
			"request": "launch",
			"type": "pwa-chrome",
			"url": "file:///${workspaceFolder}/src/index.html",
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "Launch Firefox",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"file": "${workspaceFolder}/src/index.html"
		},
		{
			"name": "Launch Edge",
			"request": "launch",
			"type": "pwa-msedge",
			"url": "file:///${workspaceFolder}/src/index.html",
			"webRoot": "${workspaceFolder}"
		}
	]
}
